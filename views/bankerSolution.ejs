<%- include('./includes/head.ejs') %>
<style>
  body {
    background-image: url("imgs/image.png");
  }
</style>
<div style="width: 100%; display: inline-flex">
  <div style="text-align: left; width: 70px">
    <button
      style="background: transparent; border: none; margin: 10px 0 0 10px"
    >
      <a href="/banker's-algo"
        ><img
          src="data:image/png;base64,"
      /></a>
    </button>
  </div>

  <div class="center" style="width: fit-content; margin-left: 200px">
    <h1 class="headings" style="margin-top: 50px">BANKER'S ALGORITHM</h1>
  </div>
</div>
<% if(isDeadLock){ %>

<div class="center" style="margin-top: 200px">
  <h1 class="solution-heading" style="color: white; font-size: 70px">
    DEADLOCK
  </h1>
</div>
<% } else{ %>
<h1 style="color: white; text-decoration: underline #dd2682" class="center">
  Process Sequence
</h1>
<div class="solution-heading">
  <% for (var i=0; i < processSequence.length; i++) { %>
  <h1 style="margin: 20px 20px auto; color: white">
    <%= processSequence[i] %>
  </h1>
  <% } %>
</div>
<h1 style="color: white; margin-bottom: 50px; font-size: 50px" class="center">
  NO DEADLOCK!
</h1>
<% } %> <%- include('./includes/end.ejs') %>
