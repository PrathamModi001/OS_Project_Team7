<%- include('./includes/head.ejs') %>

    <div class="center">
        <h1 class="headings">MRU SOLUTION</h1>
    </div>

    <div class="container" style="text-align: center">
        <table class="table table-striped table-dark">
            <thead>
                <tr>
                    <th>Page</th>
                    <% for (var i=0; i < refString.length; i++) { %>
                        <th>
                            <%= refString[i] %>
                        </th>
                        <% } %>
                </tr>
            </thead>
            <tbody>
                <% for (var j=0; j < frame; j++) { %>
                    <tr>
                        <td>
                            <%= j %>
                        </td>
                        <% for (var k=0; k < refString.length; k++) { %>


                            <td>
                                <% for(let l=0; l<totalPages[0].length; l++){ %>
                                    <% for(let m=0;m <totalPages.length; m++){ %>
                                        <%= totalPages[m][l] %>
                                    <% } %>
                                <% } %>
                            </td>



                            <% } %>
                    </tr>
                    <% } %>
            </tbody>
        </table>
    </div>
    <h1>
        <%= hit %>
    </h1>
    <h1>
        <%= pageFault %>
    </h1>
    <%- include('./includes/end.ejs') %>