<%- include('./includes/head.ejs') %>

<style>
    body{
      background-image: url("imgs/image.png");
    }
  </style>

<div style="width: 100%; display: inline-flex;">
    <div style="text-align: left;width: 70px; z-index: 3; ">
      <button style="background: transparent; border: none; margin: 10px 0 0 10px;"><a href="/mru"><img src="data:image/png;base64,"></a></button>
    </div>
  
    <div class="center" style="width: fit-content; margin-left: -30px;">
      <h1 class="headings" style="margin-top: 30px;">MRU PAGE REPLACEMENT ALGORITHM</h1>
    </div>
  </div>

    <div class="container" style="display: flex; margin-bottom: 20px; text-align: center;">
        <div style="background-color: red; margin-left: 475px;" class="colorBox"></div>
        <h3 class="solution-subHeading">Page Fault</h1>
        <div style="background-color: green; margin-left: 50px;" class="colorBox"></div>
        <h3 class="solution-subHeading">Hit</h1>
    </div>

    <div class="container" style="text-align: center">
        <table class="table table-striped table-dark" style="border: transparent;">
            <thead>
                <tr>
                    <th style="font-weight: 900; width: 104.98px; font-size: large;">Page</th>
                    <% for (var i=0; i < refString.length; i++) { %>
                        <th style="font-weight: 900; font-size: large;">
                            <%= refString[i] %>
                        </th>
                        <% } %>
                </tr>
            </thead>
        </table>
        <table class="table table-striped table-dark" style="border: transparent;">
            <tbody>
                <% for (var j=0; j < frame; j++) { %>
                    <tr>
                        <th style="font-weight: 600; width: 104.98px; font-size: large;">
                            <%= j+1 %>
                        </th>
                        <% for (var k=0; k < refString.length; k++) { %>
                            <% if(didHit[k]===true) { %>
                            <td style="color: green; font-weight: 900; font-size: large;">
                                <%= totalPages[k][j] %>
                                
                            </td>
                            <% continue; %>
                            <% } %>
                            <td style="color: red; font-weight: 900; font-size: large;">
                                <%= totalPages[k][j] %>
                            </td>
                            <% } %>
                    </tr>
                    <% } %>
            </tbody>
        </table>
    </div>
    <div style="display: inline-flex;">
    <h3 style="margin: 50px;margin-left: 80px; margin-right: 15px;color: white;" class="center">
        <a class="solution-subHeading">Hits:</a>
    <%=hit%>
     <span style="margin: auto 10px;">/</span>
    <%= ((hit/(hit+pageFault))*100).toFixed(2) %>%
    </h3>
    <h3 style="margin: 50px;margin-left: 250px; margin-right: 15px; color: white;" class="center">
        <a class="solution-subHeading">Page Faults:</a>
    <%=pageFault%>
    <span style="margin: auto 10px;">/</span>
    <%= ((pageFault/(hit+pageFault))*100).toFixed(2) %>%
    </h3>
    </div>
    <%- include('./includes/end.ejs') %>